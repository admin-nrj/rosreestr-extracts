{
  "name": "proto",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/proto/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project proto --web",
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/libs/interfaces/src"
      ],
      "options": {
        "command": "npx protoc --ts_proto_out=./libs/interfaces/src/ --proto_path=./libs/proto/src/ ./libs/proto/src/*.proto --ts_proto_opt=nestJs=true"
      }
    }
  }
}
