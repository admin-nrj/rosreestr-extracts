    "migration:show": "npm run typeorm -- migration:show -d libs/database/src/data-source.ts",
    "proto:generate": "npx protoc --ts_proto_out=./libs/interfaces/src/ --proto_path=./libs/proto/src/ ./libs/proto/src/*.proto --ts_proto_opt=nestJs=true"
